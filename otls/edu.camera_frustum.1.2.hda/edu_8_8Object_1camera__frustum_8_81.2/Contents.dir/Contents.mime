{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"// get camera and information
string cam = chs(\"camera\");
float near = chf(cam+\"/near\");
float far  = chf(cam+\"/far\");

// add padding
vector padlo = chv(\"lower_padding\")/100;
vector padup = chv(\"upper_padding\")/100;
vector pad = set(1,1,1) + set(padup.x, padup.y, 0) + set(padlo.x, padlo.y, 0);
v@P *= pad;

// normalize position for NDC
v@P += set(0.5*(1+(padup-padlo).x), 0.5*(1+(padup-padlo).y), -0.5);

// use near/far clipping
// solution A: percentage of respectively near and far
// v@P.z *= far - near*(1-padlo.z) + (far-near)*padup.z;
// v@P.z -= near*(1-padlo.z); 

// solution B: percentage of the original frustum length: flength = far - near;
float flength = far - near;
v@P.z *= (far + flength*padup.z) - (near - flength*padlo.z);
v@P.z -= max(0, near - (far-near)*padlo.z); // pad with percentage of frustum length, but not beyond the camera

// build frustum with NDC
v@P = fromNDC(cam, v@P);
"	)
